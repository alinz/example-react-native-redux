{
  "name": "Counter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "nyc mocha --opts ./app/tests/mocha.opts --compilers js:babel-core/register --recursive ./app/tests/*/*.js ./app/tests/*/*/*.js",
    "setup_project": "npm run step1; npm run step2",
    "step1": "rm -rf node_modules/react-native/node_modules/react; rm -rf node_modules/react-native/node_modules/fbjs; rm -rf node_modules/react/node_modules/fbjs",
    "step2": "cd node_modules; find . -name .babelrc | grep -v packager | xargs rm",
    "testjest": "jest",
    "testjest_update": "jest -u"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "dependencies": {
    "react": "15.2.1",
    "react-native": "^0.31.0",
    "react-redux": "4.4.0",
    "redux": "3.3.1",
    "redux-thunk": "1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-jest": "^14.1.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "jest": "^14.1.0",
    "jest-react-native": "^14.1.3",
    "nyc": "^8.1.0",
    "react-dom": "^15.3.1",
    "react-native-mock": "^0.2.6",
    "react-test-renderer": "^15.3.1"
  }
}
